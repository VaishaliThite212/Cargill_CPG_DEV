    <fields>
        <fullName>KGUnitPrice__c</fullName>
        <description>Field is created as a part of Cargill Leap Project</description>
        <externalId>false</externalId>
        <formula>CASE(UnitOfMeasure__c,
   &quot;Pound (lb)&quot;, TotalPrice / KGUnitQuantity__c, 
   &quot;Kilogram (kg)&quot;, UnitPrice, 
   &quot;Hundred Weight (CWT)&quot;, TotalPrice /(KGUnitQuantity__c * 50.8023),
   &quot;Metric Ton (MT)&quot;, TotalPrice / (KGUnitQuantity__c * 2205),
   &quot;Each (EA)&quot;, IF(PricebookEntry.Product2.EAtoKGConversionRate__c &lt;&gt; 0, TotalPrice / PricebookEntry.Product2.EAtoKGConversionRate__c,0),
   &quot;Gallon (GAL)&quot;, IF(PricebookEntry.Product2.GALtoKGConversionRate__c &lt;&gt; 0, TotalPrice /  PricebookEntry.Product2.GALtoKGConversionRate__c, 0),
   &quot;Case&quot;, IF(PricebookEntry.Product2.CASEtoKGConversionRate__c &lt;&gt; 0,TotalPrice / PricebookEntry.Product2.CASEtoKGConversionRate__c,0),
   &quot;Liter&quot;, IF(PricebookEntry.Product2.LITERtoKGConversionRate__c &lt;&gt; 0, Quantity * PricebookEntry.Product2.LITERtoKGConversionRate__c, 0),
   0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Kilogram (kg) Unit Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>6</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>KGUnitQuantity__c</fullName>
        <description>Field is created as a part of Cargill Leap Project</description>
        <externalId>false</externalId>
        <formula>CASE(UnitOfMeasure__c,
   &quot;Pound (lb)&quot;, Quantity * 2.20462, 
   &quot;Kilogram (kg)&quot;, Quantity, 
   &quot;Hundred Weight (CWT)&quot;, Quantity * 50.8023,
   &quot;Metric Ton (MT)&quot;, Quantity * 2205,
   &quot;Each (EA)&quot;, Quantity * PricebookEntry.Product2.EAtoKGConversionRate__c,
   &quot;Gallon (GAL)&quot;, Quantity * PricebookEntry.Product2.GALtoKGConversionRate__c,
   &quot;Case&quot;, Quantity * PricebookEntry.Product2.CASEtoKGConversionRate__c,
   &quot;Liter&quot;, Quantity * PricebookEntry.Product2.LITERtoKGConversionRate__c,
   0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Kilogram (kg) Unit Quantity</label>
        <precision>18</precision>
        <required>false</required>
        <scale>6</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>LBUnitPrice__c</fullName>
        <description>Field is created as a part of Cargill Leap Project</description>
        <externalId>false</externalId>
        <formula>CASE(UnitOfMeasure__c,
   &quot;Pound (lb)&quot;, UnitPrice, 
   &quot;Kilogram (kg)&quot;, TotalPrice / KGUnitQuantity__c, 
   &quot;Hundred Weight (CWT)&quot;, TotalPrice /(KGUnitQuantity__c *100),
   &quot;Metric Ton (MT)&quot;, TotalPrice / (KGUnitQuantity__c * 1000),
   &quot;Each (EA)&quot;, 
      IF(
      PricebookEntry.Product2.EAtoLBConversionRate__c &lt;&gt; 0,
      TotalPrice / PricebookEntry.Product2.EAtoLBConversionRate__c,
      0),
   &quot;Gallon (GAL)&quot;,
      IF(
      PricebookEntry.Product2.GALtoLBConversionRate__c &lt;&gt; 0, 
      TotalPrice /  PricebookEntry.Product2.GALtoLBConversionRate__c,
      0),
   &quot;Case&quot;, 
       IF(
       PricebookEntry.Product2.CASEtoLBConversionRate__c &lt;&gt; 0, 
       TotalPrice / 
       PricebookEntry.Product2.CASEtoLBConversionRate__c,
       0),
   &quot;Liter&quot;, 
        IF(
        PricebookEntry.Product2.LITERtoLBConversionRate__c &lt;&gt;0,
        Quantity *  
        PricebookEntry.Product2.LITERtoLBConversionRate__c,
        0),
   0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Pound (lb) Unit Price</label>
        <precision>18</precision>
        <required>false</required>
