&amp; 
IMAGE(&quot;/resource/impframework__CloudGURU/images/progress/blue.png&quot;, &quot;Progress&quot;, 12, 1) 

&amp; &quot; &quot; 
&amp; TEXT( ROUND (IF(Completed__c&gt;1,1,Completed__c) * 100, 0) )
&amp; &quot;%&quot;</formula>
