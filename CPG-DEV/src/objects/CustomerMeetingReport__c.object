        <fullName>Customer_Meeting_Date_Blank_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
OR(ISPICKVAL(Status__c, &quot;Closed&quot;), 
ISPICKVAL(Status__c, &quot;Complete&quot;) 
), 
ISNULL(MeetingDate__c) 
)</errorConditionFormula>
