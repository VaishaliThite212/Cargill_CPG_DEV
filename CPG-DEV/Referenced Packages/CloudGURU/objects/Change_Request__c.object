        <errorMessage>Only change requests which are part of an active sprint can be progressed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Percent_Complete_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>OR ( ISNULL( Completed__c ), 
