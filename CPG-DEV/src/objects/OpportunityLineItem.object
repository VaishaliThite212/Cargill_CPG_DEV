        <externalId>false</externalId>
        <formula>CASE(UnitOfMeasure__c,
   &quot;Pound (lb)&quot;, Quantity, 
   &quot;Kilogram (kg)&quot;, Quantity * 0.453592, 
   &quot;Hundred Weight (CWT)&quot;, Quantity * 100,
   &quot;Metric Ton (MT)&quot;, Quantity * 1000,
   &quot;Each (EA)&quot;, Quantity * PricebookEntry.Product2.EAtoLBConversionRate__c,
   &quot;Gallon (GAL)&quot;, Quantity * PricebookEntry.Product2.GALtoLBConversionRate__c,
   &quot;Case&quot;, Quantity * PricebookEntry.Product2.CASEtoLBConversionRate__c,
   &quot;Liter&quot;, Quantity * PricebookEntry.Product2.LITERtoLBConversionRate__c,
   0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
