    <validationRules>
        <fullName>Block_Closed_Won_with_Inactive_Products</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
       InactiveProducts2__c &gt; 0,
   ISPICKVAL( StageName , &apos;Closed Won&apos;),
    NOT(ISPICKVAL( PRIORVALUE(StageName), &apos;Closed Won&apos; ))
   )</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Opportunity Validation Error:  Cannot update Stage to &quot;Closed Won
 if there are INACTIVE Products associated with the Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ClosedLostReason</fullName>
        <active>true</active>
        <description>User should be forced to enter a closed-lost reason when an opportunity is marked as closed</description>
        <errorConditionFormula>ISPICKVAL(StageName, &apos;Closed Lost&apos;)  &amp;&amp;  ISPICKVAL( CloseReason__c ,&apos;&apos;)</errorConditionFormula>
        <errorDisplayField>CloseReason__c</errorDisplayField>
        <errorMessage>Opportunity Close Validation Error: Closed Reason required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IdentifyQuantityOfDealThatWasWonLost</fullName>
        <active>true</active>
        <description>Identify the Quantity of the deal that was Won/Lost</description>
        <errorConditionFormula>AND(
	PartiallyWon__c,
	ISBLANK( TotalOpportunityQuantity )
)</errorConditionFormula>
        <errorDisplayField>TotalOpportunityQuantity</errorDisplayField>
        <errorMessage>Identify the Quantity of the deal that was Won/Lost.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Closed_Opps_from_Edit</fullName>
        <active>true</active>
        <description>Prevent Users from Editing previously Closed Opps.</description>
        <errorConditionFormula>AND(
   RecordType.Name = &quot;Closed Opportunity&quot;,
   $Profile.Name &lt;&gt; &quot;System Administrator&quot;
   )</errorConditionFormula>
        <errorMessage>Opportunity Validation Error: Previously CLOSED Opportunities are LOCKED from further Edit.  You may wish to Clone the Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Products_Closed_Won_Opps</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   IsClosed ,
   ISCHANGED( NumberOfProducts__c ),
   $User.ProfileId &lt;&gt; &quot;00e4100000189Zr&quot;,  /* System Admin Profile */
   $User.ProfileId &lt;&gt; &quot;00e4C000000HgpC&quot;   /* Group Coordinator Profile */
   )</errorConditionFormula>
        <errorMessage>Opportunity Product Validation Error:  Opportunity Products are LOCKED from edit on Closed Won Opportunities.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OpportunityRestrictionToUniversalBillTo</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(
    AND(
        Account.RecordType.Name = &apos;Sold To&apos;,
        Account.Is_Universal__c = TRUE 
   
    )
)</errorConditionFormula>
        <errorMessage>Opportunity can be associated to Universal Bill To level only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OpportunityStageProcessValidation</fullName>
        <active>true</active>
        <errorConditionFormula>CASE( StageName , 
&quot;Explore&quot;,1, 
&quot;Discover&quot;,2, 
&quot;Closed Won&quot;,3, 
&quot;Closed Lost&quot;,4,0) 
&lt; 
CASE(PRIORVALUE(StageName) , 
&quot;Explore&quot;,1, 
&quot;Discover&quot;,2, 
&quot;Closed Won&quot;,3, 
&quot;Closed Lost&quot;,4,0)</errorConditionFormula>
        <errorMessage>Opportunity Stage cannot changes to earlier stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RestrictOpportunityCreationFromLead</fullName>
        <active>true</active>
        <description>Opportunity cannot be created from Lead (i.e. lead conversion process ).</description>
        <errorConditionFormula>AND(
   ISNEW(), 
   IsSourceLeadConversion__c 

)</errorConditionFormula>
        <errorMessage>Lead cannot be converted to Opportunity. Please keep opportunity selection blank.</errorMessage>
    </validationRules>
</CustomObject>
