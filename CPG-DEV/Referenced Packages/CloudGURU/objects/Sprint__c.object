    <validationRules>
        <fullName>Sprint_end_date_in_the_past</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
    End_Date__c  &lt; TODAY(),  
    ISPICKVAL(Status__c, &quot;Completed&quot;)
)</errorConditionFormula>
        <errorMessage>The sprint start date is in the future. Please change the end date before you can finish the sprint</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sprint_start_date_in_the_future</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
    Start_Date__c  &gt; TODAY(),  
    ISPICKVAL(Status__c, &quot;In Progress&quot;)
)</errorConditionFormula>
        <errorMessage>The sprint start date is in the future. Please change the start date before you can start the sprint</errorMessage>
    </validationRules>
