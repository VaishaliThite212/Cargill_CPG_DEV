    <validationRules>
        <fullName>ProjectWithOpportunityStageClosedWon</fullName>
        <active>false</active>
        <description>If the related Opportunity is in Closed Won stage, then no update is allowed</description>
        <errorConditionFormula>ISPICKVAL(Opportunity__r.StageName , &apos;Closed Won&apos;)</errorConditionFormula>
        <errorMessage>If the related Opportunity is in Closed Won stage, then no update is allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_with_Opportunit_with_no_Products</fullName>
        <active>true</active>
        <description>Throw an error on Project if the related Opportunity does not have any Products associated</description>
        <errorConditionFormula>Opportunity__r.NumberOfProducts__c = 0</errorConditionFormula>
        <errorDisplayField>Opportunity__c</errorDisplayField>
        <errorMessage>Please select the opportunity which has products associated with it.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Approval_to_Advance_Stage</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
    isApproved__c = FALSE,
    NOT(ISPICKVAL(Stage__c, &quot;Draft&quot;))
   )</errorConditionFormula>
        <errorDisplayField>Stage__c</errorDisplayField>
        <errorMessage>Project Validation Error: Project Approval is required to advance Stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RestrictProjectCreationOnOppAccRecType</fullName>
        <active>true</active>
        <description>Project can only be created when related opportunity&apos;s account record type is &quot;Prospect or Global Account or Regional Account Group&quot;.</description>
        <errorConditionFormula>AND(Opportunity__r.Account.RecordType.Name  &lt;&gt; &apos;Global Account&apos;,
Opportunity__r.Account.RecordType.Name  &lt;&gt; &apos;Prospect&apos;,
Opportunity__r.Account.RecordType.Name  &lt;&gt; &apos;Country Account Group&apos;)</errorConditionFormula>
        <errorDisplayField>Opportunity__c</errorDisplayField>
        <errorMessage>Projects can only be created if the Opportunity ACCOUNT is a Prospect or Global Account or Regional Account Group Account Record Type.</errorMessage>
    </validationRules>
